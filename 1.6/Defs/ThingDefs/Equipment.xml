	<ThingDef ParentName="BaseArtilleryBuilding">
		<defName>MV_Turret_GrapplingLauncher</defName>
		<label>grappling launcher</label>
		<description>TODO: A turret that sets up a zipline at the location where it pierces. Used for direct access between vehicles.</description>
		<thingClass>VehicleMapFramework.Building_TurretGunForcedTargetOnly</thingClass>
		<designationCategory>VMF_VehicleMap</designationCategory>
		<designatorDropdown>MV_GrapplingLauncherGroup</designatorDropdown>
		<graphicData>
			<texPath>MapVehiclesOcean/GrapplingLauncher/GrapplingLauncher_Base</texPath>
			<drawRotated>false</drawRotated>
			<drawSize>1.05</drawSize>
		</graphicData>
		<size>(1,1)</size>
		<stuffCategories Inherit="False"/>
		<costStuffCount Inherit="False"/>
		<costList>
			<WoodLog>50</WoodLog>
			<Steel>15</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<hasInteractionCell>false</hasInteractionCell>
		<inspectorTabs Inherit="False"/>
		<building>
			<turretGunDef>MV_Gun_GrapplingLauncher</turretGunDef>
			<turretBurstWarmupTime>0</turretBurstWarmupTime>
			<turretBurstCooldownTime>0</turretBurstCooldownTime>
			<turretTopDrawSize>1.25</turretTopDrawSize>
			<relatedBuildCommands>
				<li>MV_MooringRing</li>
			</relatedBuildCommands>
		</building>
		<statBases>
			<MaxHitPoints>90</MaxHitPoints>
			<Flammability>0.75</Flammability>
			<WorkToBuild>1200</WorkToBuild>
			<Mass>20</Mass>
			<Beauty>-20</Beauty>
			<ShootingAccuracyTurret>1</ShootingAccuracyTurret>
		</statBases>
		<comps Inherit="False">
			<li Class="CompProperties_Forbiddable"/>
			<li Class="CompProperties_Interactable">
				<compClass>CompInteractableRocketswarmLauncher</compClass>
				<activeTicks>1</activeTicks>
				<ticksToActivate>90</ticksToActivate>
				<jobString>activate</jobString>
				<activateTexPath>UI/Commands/ActivateTurret</activateTexPath>
				<targetingParameters>
					<canTargetBuildings>false</canTargetBuildings>
					<canTargetAnimals>false</canTargetAnimals>
					<canTargetMechs>false</canTargetMechs>
					<onlyTargetControlledPawns>true</onlyTargetControlledPawns>
				</targetingParameters>
			</li>
			<li Class="VehicleMapFramework.CompProperties_Zipline">
				<standbyGraphic>
					<texPath>MapVehiclesOcean/GrapplingLauncher/GrapplingLauncher_TopOverlay</texPath>
					<graphicClass>Graphic_Single</graphicClass>
					<drawSize>1.25</drawSize>
				</standbyGraphic>
			</li>
		</comps>
		<researchPrerequisites>
		</researchPrerequisites>
		<uiOrder>1063</uiOrder>
		<uiIconPath>MapVehiclesOcean/GrapplingLauncher/GrapplingLauncher_MenuIcon</uiIconPath>
		<placeWorkers Inherit="False">
			<li>PlaceWorker_TurretTop</li>
		</placeWorkers>
	</ThingDef>

